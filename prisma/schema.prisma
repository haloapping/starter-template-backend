generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

generator dbml {
  provider = "prisma-dbml-generator"
}

generator zod {
  provider = "zod-prisma-types"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id
  username  String   @unique
  createdAt DateTime
  updatedAt DateTime @updatedAt
}
